{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "description":"A video in pytube/data",
  "properties":{
    "alias":{
      "type":"string"
    },
    "category":{
      "type":"string"
    },
    "copyright_text":{
      "type":"string"
    },
    "description":{
      "type":"string"
    },
    "duration":{
      "type":"integer"
    },
    "id":{
      "description":"A unique identifier for a particular video",
      "type":"integer"
    },
    "language":{
      "type":"string"
    },
    "quality_notes":{
      "type":"string"
    },
    "recorded":{
      "format":"date",
      "type":"string"
    },
    "slug":{
      "type":"string"
    },
    "source_url":{
      "format":"uri",
      "type":"string"
    },
    "speakers":{
      "items":{
        "type":"string"
      },
      "minItems":1,
      "type":"array",
      "uniqueItems":true
    },
    "summary":{
      "type":"string"
    },
    "tags":{
      "items":{
        "type":"string"
      },
      "type":"array",
      "uniqueItems":true
    },
    "thumbnail_url":{
      "format":"uri",
      "type":"string"
    },
    "title":{
      "type":"string"
    },
    "videos":{
      "description":"Video location records",
      "items":{
        "description":"A video location record",
        "properties":{
          "type":{
            "type":"string"
          },
          "url":{
            "format":"uri",
            "type":"string"
          }
        },
        "required":[
          "url"
        ],
        "type":"object"
      },
      "minItems":1,
      "type":"array",
      "uniqueItems":true
    }
  },
  "required":[
    "title",
    "source_url",
    "speakers"
  ],
  "title":"Video",
  "type":"object"
}