{
  "$schema":"http://json-schema.org/draft-04/schema#",
  "description":"The schema for a video record in pytube/data",
  "properties":{
    "alias":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "category":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "copyright_text":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "description":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "duration":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "integer"}]
    },
    "id":{
      "description":"A unique identifier for a particular video",
      "anyOf": [{"type": "null"}, {"type": "integer"}]
    },
    "language":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "quality_notes":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "recorded":{
      "format":"date-time",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "slug":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "source_url":{
      "format":"uri",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "speakers":{
      "description":"",
      "items":{
        "description":"",
        "type":"string"
      },
      "type":"array",
      "uniqueItems":true
    },
    "summary":{
      "description":"",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "tags":{
      "description":"",
      "items":{
        "description":"",
        "type":"string"
      },
      "type": "array",
      "uniqueItems":true
    },
    "thumbnail_url":{
      "description":"",
      "format":"uri",
      "anyOf": [{"type": "null"}, {"type": "string"}]
    },
    "title":{
      "description":"",
      "type":"string"
    },
    "videos":{
      "description":"Array of video locations",
      "items":{
        "description":"A video location",
        "properties":{
          "type":{
            "type":"string"
          },
          "url":{
            "format":"uri",
            "type":"string"
          },
          "length":{
            "anyOf": [{"type": "null"}, {"type": "integer"}]
          }
        },
        "required":[
          "url"
        ],
        "type":"object"
      },
      "type":"array",
      "uniqueItems":true
    }
  },
  "required":[
    "title",
    "speakers",
    "videos"
  ],
  "title":"Video",
  "type":"object"
}
